{
  "name": "victor-blog",
  "version": "1.0.0",
  "description": "A basic CRUD blogging website",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run dev:api\" \"npm run dev:web\"",
    "dev:api": "cd apps/server && npm run dev",
